{# app/Resources/views/app/postShow.html.twig #}
{% extends 'app/layout.html.twig' %}

{% block title %}
  {{ post.title }} - {{ parent() }}
{% endblock %}

{% block bodySite %}
    <h1>{{ post.title | upper }}</h1>
        <h2>{{ post.authorEmail }}</h2>
            <p>{{ post.content | raw }}</p>
{% endblock %}